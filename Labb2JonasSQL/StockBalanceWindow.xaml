<Window x:Class="Labb2JonasSQL.StockBalanceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Stock Balance" Height="300" Width="400">
    <Grid>
        
        <DataGrid x:Name="stockBalanceDataGrid" AutoGenerateColumns="False">
    <DataGrid.Columns>
        <DataGridTextColumn Header="Store ID" Binding="{Binding StoreId}" />
        <DataGridTextColumn Header="ISBN" Binding="{Binding ISBN13}" />
        <DataGridTextColumn Header="Quantity" Binding="{Binding Quantity}" />
        
    </DataGrid.Columns>
</DataGrid>

    </Grid>
</Window>
